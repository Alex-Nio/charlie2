{
  "name": "charlie-template",
  "description": "Charlie template",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run start:server\" \"cross-env VITE_NETWORK_PROCESSOR=tauri vite\" && tauri dev",
    "serve": "vite",
    "serve:tauri": "cross-env VITE_NETWORK_PROCESSOR=tauri vite",
    "serve:native": "concurrently \"npm run serve:tauri\" \"tauri dev\"",
    "build": "cross-env VITE_NETWORK_PROCESSOR=tauri vite build && tauri build",
    "preview": "vite preview",
    "start:server": "npm run activate-venv && node ../backend/server.js",
    "activate-venv": "..\\backend\\venv\\Scripts\\activate"
  },
  "dependencies": {
    "@mdi/font": "7.2.96",
    "@tauri-apps/api": "1.4.0",
    "axios": "^1.6.1",
    "roboto-fontface": "0.10.0",
    "unified-network": "0.6.4",
    "vue": "3.3.4",
    "vuetify": "3.3.7"
  },
  "devDependencies": {
    "@tauri-apps/cli": "1.4.0",
    "@vitejs/plugin-vue": "4.0.0",
    "concurrently": "^8.2.2",
    "cross-env": "7.0.3",
    "sass": "1.63.6",
    "vite": "4.4.2",
    "vite-plugin-vuetify": "1.0.2"
  },
  "main": "index.js",
  "repository": "git@github.com:yooneskh/vite-vuetify-template.git",
  "author": "Yoones Khoshghadam <yooneskh@gmail.com>",
  "license": "MIT"
}
